{
  "name": "pronohub",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "iOS >= 15",
    "Android >= 10"
  ],
  "scripts": {
    "dev": "next dev -p 3100",
    "dev:clean": "rmdir /s /q .next 2>nul & next dev -p 3100",
    "build": "next build",
    "build:android": "npm run build && npx cap sync android",
    "android": "npm run build:android && cd android && .\\gradlew assembleDebug",
    "start": "next start",
    "lint": "next lint",
    "kill": "for /f \"tokens=5\" %a in ('netstat -ano ^| findstr :3100 ^| findstr LISTENING') do taskkill /F /PID %a"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@capacitor/android": "^8.0.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/push-notifications": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@codetrix-studio/capacitor-google-auth": "^3.4.0-rc.4",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.78.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "dotenv": "^17.2.3",
    "emoji-picker-react": "^4.12.0",
    "firebase": "^12.8.0",
    "firebase-admin": "^13.6.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.552.0",
    "next": "^16.0.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "resend": "^6.5.2",
    "satori": "^0.12.0",
    "stripe": "^20.0.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.16"
  }
